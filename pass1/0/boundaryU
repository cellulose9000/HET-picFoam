FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

//velocity boundary conditions -> inlet velocity = 300m/s from back, -z

boundaryField
{
    backBoundary
    {
        type            fixedValue;
        value           uniform (0 0 -300); //or (0 0 -4000)
    }
}


