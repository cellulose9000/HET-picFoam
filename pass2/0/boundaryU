FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    cathode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    anode
    {
        type            fixedValue;
        value           uniform (22e2 0 0);
    }
    InnerPole
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    OuterPole
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    freeSpace
    {
        type            zeroGradient;
    }
    centerline
    {
        type            symmetryPlane;
    }
}


