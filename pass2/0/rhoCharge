FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rhoCharge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    anode
    {
        type            calculated;
        value           uniform 0;
    }
    cathode
    {
        type            calculated;
        value           uniform 0;
    }
    freeSpace
    {
        type            zeroGradient;
    }
    centerline
    {
        type            symmetryPlane;
    }
}


