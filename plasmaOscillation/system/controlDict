FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     picFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         8e-8;

deltaT          1e-11;

writeControl    runTime;

writeInterval   2e-9;

cycleWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable true;


